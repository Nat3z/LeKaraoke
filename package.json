{
  "version": "0.1.0",
  "name": "electron-quick-start",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch \"./**\" --ext ts html --exec \"yarn run start\"",
    "dev:server": "nodemon --watch \"src/**\" --ext ts --exec \"tsc && node ./dist/server.js\"",
    "start": "npm run build && electron ./dist/main.js"
  },
  "devDependencies": {
    "@types/electron-localshortcut": "^3.1.3",
    "@types/node": "^20.11.16",
    "electron": "^28.2.1",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/ip": "^1.1.3",
    "electron-localshortcut": "^3.2.1",
    "express": "^4.18.2",
    "ip": "^2.0.1",
    "puppeteer": "^21.11.0",
    "youtubei.js": "^10.0.0"
  }
}
